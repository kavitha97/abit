<%=form_tag home_index_path,multipart:true do%>
<%=file_field_tag :file%>
<%=submit_tag ('importcsv')%>
<%end%>
<h1> Student information </h1>
<table>
<th>Rollno</th>
<th>Name</th>
<th>Grade</th>
<th>Branch</th>
<tr>
<%@s.each do |s|%>
<td><%=s.rollno%></td>
<td><%=s.name%></td>
<td><%=s.grade%></td>
<td><%=s.branch%></td>
</tr>
<%end%>
</table>